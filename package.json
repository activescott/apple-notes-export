{
  "name": "apple-notes-export",
  "version": "0.0.1",
  "private": true,
  "license": "MPL-2.0",
  "scripts": {
    "build": "rm -f ./dist/*.* && webpack  && echo ''",
    "start": "npm run build && osascript -l JavaScript dist/bundle.js",
    "eslint": "./node_modules/.bin/eslint . --ext ts,tsx,js,jsx",
    "lint": "./node_modules/.bin/prettier -l \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint",
    "lint-fix": "./node_modules/.bin/prettier --write \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint --fix"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "agentmarkdown": "^4.2.2",
    "core-js": "3",
    "mime-types": "^2.1.26",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@activescott/eslint-config": "^1.1.2",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@types/mime-types": "^2.1.0",
    "@types/minimist": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "description": "A script for programmatically exporting your data from the Apple Notes application.",
  "author": "Scott Willeke <scott@willeke.com>"
}
